{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "importsNotUsedAsValues": "preserve",
    "isolatedModules": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["sidebar/*"]
    }
  },
  "include": ["sidebar/**/*", "vite.config.mjs"],
  "exclude": ["node_modules"]
}
